pipeline{
   agent any
          stages {
                  stage('SCM Checkout')
                   {git 'https://github.com/Mukesh889/maven-project.git' }
                  stage ('Installing the code')
                      {
                        steps{
                            withMaven(maven: 'LocalMaven')
                              { sh 'mvn install'}
                         }
                       }
                  }
               }
