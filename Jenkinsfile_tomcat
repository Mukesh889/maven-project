pipeline {
    agent any
    stages {
        stage('SCM Checkout'){
        steps{
          git 'https://github.com/Mukesh889/maven-project'
        }
        }
 
   stage('Packaging with sonar'){
   stpes{
     withSonarQubeEnv('sonar-pipeline'){
     sh 'mvn sonar:sonar clean package'
     }
     }
    }
   }
}
